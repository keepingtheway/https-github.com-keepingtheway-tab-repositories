public class GirlFriend {
		private String name;
		private int age;
		public String getName() {
			return name;
		}
		public GirlFriend(){}
		
		public GirlFriend(String name,int age){
			//this.setName(name);
			this(name);//this()调用构造方法 必须在第一句 后面不能再有
			this.setAge(age);
		}
		
		public GirlFriend(String name){
			this.name=name;
		}
		
		
		public GirlFriend(int age) {
			// TODO Auto-generated constructor stub
			this.setAge(age);
		}
		
		public void setName(String name) {
			this.name = name;
		}
		
		public void show(){
			System.out.println("姓名："+name+"年龄"+age);
		}
		public void setAge(int age){
			if(age<18){
				this.age=18;
				System.out.println("该女孩不满18周岁不要祸害小女生");
			}else{
				this.age=age;
			}
		}
		public int getAge(){
			return age;
		}
		
		
}
